# Firebase Configuration Template
# Copy this file to .env and replace with your actual Firebase project credentials

VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id-here
VITE_FIREBASE_APP_ID=your-app-id-here
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id-here

# Webhook Configuration
# This key is required for authenticating with the image generation webhook
VITE_WEBHOOK_API_KEY=your-webhook-api-key-here